<div class="product-detail">
  <img src="<%= product.image %>" alt="<%= product.name %>" />
  <div class="details">
    <h1><%= product.name %></h1>
    <p class="price">$<%= product.price %></p>
    <p><%= product.description %></p>
    <button class="btn-primary">Add to Cart</button>
  </div>
</div>
